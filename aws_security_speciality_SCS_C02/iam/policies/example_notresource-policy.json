{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "dynamodb:GetItem",
                "dynamodb:PutItem",
                "dynamodb:UpdateItem"
            ],
            "NotResource": "arn:aws:dynamodb:us-west-2:123456789012:table/RestrictedTable"
        }
    ]
}